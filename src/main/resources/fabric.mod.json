{
  "schemaVersion": 1,
  "id": "updateinator",
  "version": "${version}",

  "name": "Updateinator",
  "description": "An automatic update manager for fabric server mods!",
  "authors": [
    "SquarePear"
  ],
  "contact": {
    "homepage": "https://github.com/SquarePear/updateinator",
    "sources": "https://github.com/SquarePear/updateinator"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.reygames.updateinator.Updateinator"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
